{
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git@latest", "--repository", "."],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "defang": {
      "command": "uvx",
      "args": ["defang-mcp-server@latest"],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    }
  }
}